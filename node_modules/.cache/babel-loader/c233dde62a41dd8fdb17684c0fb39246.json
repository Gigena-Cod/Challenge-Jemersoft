{"ast":null,"code":"import request from \"@/data/request\";\nexport default {\n  name: 'PokemonView',\n\n  data() {\n    return {\n      pokemon: {},\n      loading: true\n    };\n  },\n\n  async mounted() {\n    this.pokemon = await request.obtenerPokemonPorId(this.$route.params.id);\n    this.loading = false;\n    this.getImages();\n  },\n\n  created() {},\n\n  methods: {\n    back() {\n      this.$router.go(-1);\n    },\n\n    capitalizeString(string) {\n      let newString = string[0].toUpperCase() + string.slice(1);\n      return newString;\n    },\n\n    generatorTypesNames(parameter) {\n      let typesNames = [];\n      parameter.map(types => {\n        typesNames.push(this.capitalizeString(types.type.name));\n      });\n      return typesNames;\n    },\n\n    generatorAbilitiesNames(parameter) {\n      let abilitiesNames = [];\n      parameter.map(abilities => {\n        abilitiesNames.push(this.capitalizeString(abilities.ability.name));\n      });\n      return abilitiesNames;\n    },\n\n    getImages() {\n      let photos = [];\n      this.pokemon.sprites.back_default ? photos.push(this.pokemon.sprites.back_default) : '';\n      this.pokemon.sprites.back_female ? photos.push(this.pokemon.sprites.back_female) : '';\n      this.pokemon.sprites.back_shiny ? photos.push(this.pokemon.sprites.back_shiny) : '';\n      this.pokemon.sprites.back_ ? photos.push(this.pokemon.sprites.back_) : '';\n      this.pokemon.sprites.back_default ? photos.push(this.pokemon.sprites.back_default) : '';\n      this.pokemon.sprites.back_default ? photos.push(this.pokemon.sprites.back_default) : '';\n      console.log(photos);\n    }\n\n  },\n  filters: {\n    FormatId(id) {\n      let string = `${id}`;\n      return `N.º${string.padStart(3, \"0\")}`;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiEA;AAEA;EACAA,mBADA;;EAEAC;IACA;MACAC,WADA;MAEAC;IAFA;EAIA,CAPA;;EAQA;IACA;IACA;IACA;EACA,CAZA;;EAaAC,WAEA,CAfA;;EAgBAC;IAEAC;MACA;IACA,CAJA;;IAKAC;MACA;MACA;IACA,CARA;;IASAC;MAEA;MA